{
    "name": "triangle-lab/yii2-imap",
    "minimum-stability": "dev",
    "prefer-stable": "true",
    "description": "yii2 extension to read and process mails from IMAP and PHP",
    "keywords": ["yii2", "extension", "widget", "imap", "cron", "mail", "inbox"],
    "homepage": "https://github.com/triangle-lab/yii2-imap.git",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Roopan Valiya Veetil",
            "email": "yiioverflow@gmail.com",
            "homepage": "http://www.yiioverflow.com/"
        },
        {
            "name": "Monzon Traore",
            "email": "monzon@traore.ru",
            "homepage": "http://traore.ru/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/triangle-lab/yii2-imap"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "tlab\\imap\\": "src/"
        }
    }
}
